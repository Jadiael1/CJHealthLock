{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build",
            "command": "C:/TDM-GCC-32/bin/g++.exe",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/CJGodMode.exe",
                "-lpsapi"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}